<div class="form-text">
<h2>New Table</h2>
</div>

<div class="drop_down">
<form action="/parties" method='POST'>
</div>

<div class="form-text">
<p>	Table Number: <p>
</div>
<div class="drop_down">
	<select name="party[table_number]">
		<% index = 1 %>
		<% while index < 13 %>
			<option value="<%= index %>"><%= index %> 
			</option>
			<% index += 1 %>
		<% end %>
	</select>
</div>



<div class="form-text">
  Guests: 
</div>

 <input type="text" name='party[guests]' >


  <br>
<div class="link">
<input type="submit">
</div>
</form>







